{
  "name": "test",
  "version": "1.0",
  "description": "11",
  "manifest_version": 3,
  "action": {},
  "content_scripts": [
    {
      "matches": ["https://rezka.ag/*"],
      "css": [
        "styles.css",
        "assets/all.css"

      ],
      "js": [
        "assets/all.js",
        "content.ts"
      ]
    },
    {
      "matches": ["https://translate.google.com/*"],

      "js": [
        "content.ts"
      ]
    }



  ],
  "web_accessible_resources": [{
    "resources": ["/images/kk6.png"],
    "matches": ["<all_urls>"]
  }],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "identity",
    "webNavigation",
    "tabs",
    "notifications",
    "storage",
    "clipboardWrite",
    "clipboardRead",
    "background",
    "system.display"

  ]

}
